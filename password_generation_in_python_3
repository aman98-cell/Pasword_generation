# password creation system by importing random module

import random
def password(length): 
    pw = str() #creating an empty string
    characters = "123456789" + "abchalfiubaleg" + "#$%"
    for i in range(length):
        pw = pw + random.choice(characters)
    return pw
    
    #function call, passing the length of password 10
    password(10)
